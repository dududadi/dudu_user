page{
  height:100%;
  background-color:#f0f0f0;
}
.clearfix{
  overflow: auto;
  _height: 1%;
}
.content{
  height: 100%;
}
.section-title{
  color: #E67F02;
  background-color: #f0f0f0;
  display: block;
  padding: 28rpx 0 28rpx 20rpx;
}
view >.item{
  position: relative;
  float: left;
  width: auto;
  height: 200rpx;
  text-align: center;
  line-height: 200rpx;
  border-right: 2rpx solid #f0f0f0;
  border-bottom: 2rpx solid #f0f0f0;
  background-color: #fff;
}
.item text{
  font-size: 34rpx;
}
text.iconfont{
  position: absolute;
  top: 22%;
  line-height: initial;
  left: 0;
  right: 0;
  font-size: 48rpx;
  color: #E67F02;
}
view >.border-none{
  border: 0;
}
view >.borderR-none{
  border-right: 0;
}